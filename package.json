{
  "name": "include-fussball-de-widgets",
  "version": "3.0.0",
  "description": "A WordPress plugin for easy integration of fussball.de widgets.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ITS-Boehm/include-fussball-de-widgets.git"
  },
  "author": "IT-Service Böhm -- Alexander Böhm",
  "license": "GPL-2.0-or-later",
  "bugs": {
    "url": "https://github.com/ITS-Boehm/include-fussball-de-widgets/issues"
  },
  "homepage": "https://github.com/ITS-Boehm/include-fussball-de-widgets#readme",
  "scripts": {
    "clear": "rimraf app/dist/*",
    "build": "wp-scripts build",
    "check-engines": "wp-scripts check-engines",
    "check-licenses": "wp-scripts check-licenses --production",
    "lint:css": "wp-scripts lint-style '**/*.css'",
    "lint:js": "wp-scripts lint-js .",
    "lint:pkg-json": "wp-scripts lint-pkg-json .",
    "start": "wp-scripts start",
    "i18n:pot": "wp i18n make-pot app/dist app/dist/languages/include-fussball-de-widgets.pot --slug=include-fussball-de-widgets --domain=include-fussball-de-widgets --package-name=\"Include Fussball.de Widgets\"",
    "i18n:json": "wp i18n make-json app/dist/languages/include-fussball-de-widgets-de_DE.po --no-purge",
    "release:copy": "cpr app/dist/ .releases/include-fussball-de-widgets/trunk/ -d",
    "release:github": "cpr ./app/dist/ .github-releases/include-fussball-de-widgets -d && cd .github-releases/ && bestzip include-fussball-de-widgets-v3.0.999.zip include-fussball-de-widgets && rimraf include-fussball-de-widgets && cd ..",
    "release": "npm run release:copy && npm run release:github"
  },
  "browserslist": [
    "extends @wordpress/browserslist-config"
  ],
  "devDependencies": {
    "@wordpress/browserslist-config": "^2.6.0",
    "@wordpress/scripts": "^3.4.0",
    "autoprefixer": "^9.6.1",
    "bestzip": "^2.1.4",
    "copy-webpack-plugin": "^5.0.4",
    "cpr": "^3.0.1",
    "css-loader": "^3.2.0",
    "eslint": "^6.1.0",
    "eslint-plugin-jest": "^22.15.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "prettier-eslint": "^9.0.0",
    "prettier-stylelint": "^0.4.2",
    "rimraf": "^2.7.0",
    "sass-loader": "^7.2.0",
    "stylelint": "^10.1.0",
    "stylelint-config-wordpress": "^14.0.0",
    "stylelint-order": "^3.0.1",
    "terser-webpack-plugin": "^1.4.1"
  },
  "dependencies": {}
}
