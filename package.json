{
  "name": "include-fussball-de-widgets",
  "version": "3.2.1",
  "description": "A WordPress plugin for easy integration of fussball.de widgets.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ITS-Boehm/include-fussball-de-widgets.git"
  },
  "author": "IT-Service Böhm -- Alexander Böhm",
  "license": "GPL-2.0-or-later",
  "bugs": {
    "url": "https://github.com/ITS-Boehm/include-fussball-de-widgets/issues"
  },
  "homepage": "https://github.com/ITS-Boehm/include-fussball-de-widgets#readme",
  "scripts": {
    "clear": "rimraf dist",
    "build": "rimraf dist && webpack && yarn run i18n",
    "dev": "yarn run build && node ./tasks/cp-dev-release.js",
    "lint:css": "stylelint 'src/**/*.scss'",
    "lint:css:fix": "stylelint 'src/**/*.scss' --fix",
    "lint:js": "eslint 'src/**/*.js'",
    "lint:js:fix": "prettier-eslint \"$PWD/src/**/*.js\" --write",
    "lint": "yarn run lint:css && yarn run lint:js",
    "lint:fix": "concurrently \"yarn:lint:*:fix\"",
    "start": "wp-scripts start",
    "i18n:pot": "wp i18n make-pot dist languages/include-fussball-de-widgets.pot --slug=include-fussball-de-widgets --domain=include-fussball-de-widgets --package-name=\"Include Fussball.de Widgets\"",
    "i18n:json": "wp i18n make-json src/languages/include-fussball-de-widgets-de_DE.po --no-purge",
    "i18n:copy": "cpr src/languages dist/languages -d",
    "i18n": "yarn run i18n:json && yarn run i18n:copy",
    "release:copy": "cpr dist .releases/include-fussball-de-widgets/trunk/ -d",
    "release:github": "cpr dist .github-releases/include-fussball-de-widgets -d && cd .github-releases/ && bestzip include-fussball-de-widgets-v3.2.1.zip include-fussball-de-widgets && rimraf include-fussball-de-widgets && cd ..",
    "release": "yarn run build && yarn run release:copy && yarn run release:github",
    "test:cs": "./vendor/bin/phpcs",
    "test:phpunit": "./vendor/bin/phpunit --no-coverage",
    "test:phpunit:full": "./vendor/bin/phpunit",
    "test": "yarn run test:cs && yarn run test:phpunit:full"
  },
  "browserslist": [
    "extends @wordpress/browserslist-config"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.css": "stylelint 'src/**/*.css' --fix",
    "*.js": "prettier-eslint \"$PWD/src/**/*.js\" --write"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@wordpress/babel-preset-default": "^4.15.0",
    "@wordpress/browserslist-config": "^2.7.0",
    "@wordpress/dependency-extraction-webpack-plugin": "^2.7.0",
    "@wordpress/scripts": "^12.1.1",
    "autoprefixer": "^9.8.5",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "bestzip": "^2.1.5",
    "copy-webpack-plugin": "^6.0.3",
    "cpr": "^3.0.1",
    "css-loader": "^4.0.0",
    "eslint": "^7.5.0",
    "eslint-config-semistandard": "^15.0.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.18.2",
    "eslint-plugin-jsx-a11y": "^6.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "indent-string": "^4.0.0",
    "lint-staged": "^10.2.11",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.5",
    "prettier-eslint": "^11.0.0",
    "prettier-eslint-cli": "^5.0.0",
    "prettier-standard": "16.4.1",
    "prettier-stylelint": "^0.4.2",
    "rimraf": "^3.0.2",
    "sass-loader": "^9.0.2",
    "semistandard": "14.2.3",
    "stylelint": "^13.6.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-wordpress": "^17.0.0",
    "stylelint-order": "^4.1.0",
    "terser-webpack-plugin": "^3.0.8",
    "thread-loader": "^2.1.3",
    "webpack": "^4.44.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-livereload-plugin": "^2.3.0"
  },
  "dependencies": {
    "@yarnpkg/pnpify": "^2.1.0"
  }
}
